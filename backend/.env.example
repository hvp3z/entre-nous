# Backend Environment Variables
# Copy this file to .env and fill in your values

# Node environment
NODE_ENV=development

# Server port
PORT=3001

# Frontend URL (for CORS in production)
FRONTEND_URL=http://localhost:3000

# Google Places API Key
# Get it at: https://console.cloud.google.com/apis/credentials
GOOGLE_PLACES_API_KEY=your_google_places_api_key

# Google Maps API Key
# Get it at: https://console.cloud.google.com/apis/credentials
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Navitia API Key (for transit backup)
# Get it at: https://navitia.io/
NAVITIA_API_KEY=your_navitia_api_key

# Redis URL (optional - for caching)
# For Upstash: redis://default:password@host:port
REDIS_URL=
